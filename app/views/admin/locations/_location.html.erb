<tr>
  <td>
    <%= link_to location.id, admin_location_path(location) %>
    <div class="action">
      <%= link_to content_tag(:span, "", class: "glyphicon glyphicon-pencil"), {action: :edit, id: location.id}, class: "btn btn-primary btn-xs btn-action" %>
      <%= link_to content_tag(:span, "", class: "glyphicon glyphicon-trash"),  admin_location_path(location),  method: :delete, data: {confirm: t(".sure")}, class: "btn btn-danger btn-xs btn-action" %>
    </div>
  </td>
  <td><%= location.latitude %></td>
  <td><%= location.longitude %></td>
  <td><%= location.city %></td>
  <td><%= location.street %></td>
  <td><%= location.building %></td>
</tr>